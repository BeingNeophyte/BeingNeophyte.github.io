            "& when (@RibbonPoss = 'LeftTop')": {
              ".corner-ribbon.top-zen": {
                "top": "25px",
                "left": "-50px",
                "transform": "rotate(-45deg)",
                "-webkit-transform": "rotate(-45deg)",
                "position": "fixed"
              }
            },
            "& when (@RibbonPoss = 'RightTop')": {
              ".corner-ribbon.top-zen": {
                "top": "25px",
                "right": "-50px",
                "left": "auto",
