          "_name": "design-block",
          "_customHTML": "<section mbr-id=\"_anchor\" class=\"_customHTML\">\n \n  \n    <mbr-parameters>\n<hr><header>Social Config</header>\n      <input type=\"checkbox\" title=\"Show Social Button\" name=\"showSocial\" checked>\n      <input type=\"checkbox\" title=\"Right/Left\" name=\"showPossSocial\" condition=\"showSocial\" checked>\n      <input type=\"checkbox\" title=\"Float Over\" name=\"showFloat\" condition=\"showSocial\" checked>\n       <input type=\"color\" title=\"Float Color\" name=\"FloatColor\" value=\"#2e2e2e\" condition=\"showSocial\">\n       <input type=\"range\" title=\"Radius\" name=\"SocialRadius\" min=\"0\" max=\"25\" step=\"1\" value=\"25\" condition=\"showSocial\">\n       <input type=\"range\" title=\"Spacing\" name=\"SocialSpace\" min=\"0\" max=\"10\" step=\"1\" value=\"0\" condition=\"showSocial\">\n       <input type=\"range\" title=\"Tab\" name=\"SocialSpacing\" min=\"0\" max=\"3\" step=\"0.2\" value=\"0.6\" condition=\"showSocial\">\n      <input type=\"checkbox\" title=\"Facebook\" name=\"showFB\" condition=\"showSocial\" checked>\n      <input type=\"text\" name=\"Facebook\" title=\"Link\" value=\"https://www.facebook.com/\" condition=\"showFB&&showSocial\">\n      <input type=\"checkbox\" title=\"Twitter\" name=\"showTW\" condition=\"showSocial\">\n      <input type=\"text\" name=\"Twitter\" title=\"Link\" value=\"https://www.twitter.com/\" condition=\"showTW&&showSocial\">\n      <input type=\"checkbox\" title=\"Google\" name=\"showGG\" condition=\"showSocial\">\n      <input type=\"text\" name=\"Google\" title=\"Link\" value=\"https://www.google.com/\" condition=\"showGG&&showSocial\">\n      <input type=\"checkbox\" title=\"Linkedin\" name=\"showLK\" condition=\"showSocial\">\n      <input type=\"text\" name=\"Linkedin\" title=\"Link\" value=\"https://www.linkedin.com/\" condition=\"showLK&&showSocial\">\n      <input type=\"checkbox\" title=\"Youtube\" name=\"showYT\" condition=\"showSocial\" checked>\n      <input type=\"text\" name=\"Youtube\" title=\"Link\" value=\"https://youtube.com/glitchedvelocity?sub_confirmation=1\" condition=\"showYT&&showSocial\">\n      <input type=\"checkbox\" title=\"Instagram\" name=\"showIT\" condition=\"showSocial\">\n      <input type=\"text\" name=\"Instagram\" title=\"Link\" value=\"https://www.instagram.com/\" condition=\"showIT&&showSocial\">\n      <input type=\"checkbox\" title=\"Custom 1\" name=\"showC1\" condition=\"showSocial\" checked>\n                    <input type=\"color\" title=\"Background Color\" name=\"CustomColor1\" value=\"#6576aa\" condition=\"showC1&&showSocial\">\n      <input type=\"checkbox\" title=\"Custom 2\" name=\"showC2\" condition=\"showSocial\">\n                    <input type=\"color\" title=\"Background Color\" name=\"CustomColor2\" value=\"#01bf46\" condition=\"showC2&&showSocial\">\n\n      <header>Ribbon Config</header>\n      <input type=\"checkbox\" title=\"Show Ribbon\" name=\"Ribbon\">\n      <select title=\"Position\" name=\"RibbonPoss\" condition=\"Ribbon\">\n            <option value=\"LeftTop\">Top Left</option>\n            <option value=\"RightTop\">Top Right</option>\n            <option value=\"LeftBottom\">Bottom Left</option>\n            <option value=\"RightBottom\" selected>Bottom Right</option>\n      </select>\n       <input type=\"color\" title=\"Background Color 1\" name=\"RibbonColor\" value=\"#55b4d4\" condition=\"Ribbon\">\n        <input type=\"color\" title=\"Background Color 2\" name=\"RibbonColor1\" value=\"#000000\" condition=\"Ribbon\">\n\n      \n     <header>Float Menu</header>\n\n       <input type=\"checkbox\" title=\"Show Social Bar\" name=\"Socbar\">     \n        <input type=\"range\" inline title=\"Top\" name=\"paddingTopmenu\" min=\"0\" max=\"400\" step=\"1\" value=\"400\" condition=\"Socbar\">\n        <input type=\"range\" inline title=\"Width\" name=\"widthmenu\" min=\"200\" max=\"600\" step=\"1\" value=\"200\" condition=\"Socbar\">\n        <input type=\"range\" inline title=\"Width Hover\" name=\"widthover\" min=\"100\" max=\"1000\" step=\"1\" value=\"100\" condition=\"Socbar\">\n        <input type=\"range\" inline title=\"Thickness\" name=\"heightmenu\" min=\"43\" max=\"100\" step=\"1\" value=\"43\" condition=\"Socbar\">\n        <input type=\"color\" title=\"Background Color\" name=\"bgcolormenu\" value=\"#cccccc\" condition=\"Socbar\">\n        <input type=\"range\" inline title=\"Items Count\" name=\"iconsCount\" min=\"1\" max=\"9\" step=\"1\" value=\"5\" condition=\"Socbar\">\n        <input type=\"checkbox\" title=\"Grayscale\" name=\"Grayscale\" condition=\"Socbar\">\n      \n    </mbr-parameters> \n  <div class=\"sticky-container\" mbr-if=\"Socbar\">\n\t\t<ul class=\"sticky\">\n\n                        \t\t\t<li>\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 1</a></p>\n\t\t\t</li>\n            <li mbr-if=\"iconsCount > 1\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 2</a></p>\n\t\t\t</li>\n            \t\t\t<li mbr-if=\"iconsCount > 2\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 3</a></p>\n\t\t\t</li>\n            \t\t\t<li mbr-if=\"iconsCount > 3\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 4</a></p>\n\t\t\t</li>\n            \t\t\t<li mbr-if=\"iconsCount > 4\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" dat