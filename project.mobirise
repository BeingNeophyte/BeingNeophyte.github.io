    "Plugin_Keyboard_Shortcuts.html": {
      "settings": {
        "meta_descr": "A Coppercube Plugin for keyboard shortcuts.",
        "title": "Plugin Keyboard Shortcuts",
        "order": 8
      },
      "components": [
        {
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 15px)",
              "padding-bottom": "(@paddingBottom * 15px)"
            },
            "& when (@bg-type = 'color')": {
              "background": "linear-gradient(45deg, @bg-value, @color2)"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
